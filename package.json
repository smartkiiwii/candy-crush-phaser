{
  "name": "candy-crush",
  "version": "2.0.0",
  "description": "Candy crush for Phaser 3 in TypeScript",
  "keywords": [],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "expose-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.3",
    "javascript-obfuscator": "^4.0.2",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^5.0.1",
    "serve": "^14.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "webpack-obfuscator": "^3.5.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "phaser": "^3.60.0",
    "phaser3-rex-plugins": "^1.60.3"
  },
  "scripts": {
    "dev": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "preview": "serve dist",
    "util:check": "prettier --check src/**",
    "util:format": "prettier --write src/**"
  },
  "alias": {
    "@/": "./src"
  },
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "license": "MIT"
}
